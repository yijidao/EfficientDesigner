<hc:Window x:Class="EfficientDesigner_Shell.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EfficientDesigner_Shell.Views"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="可视化设计器Shell" Height="1080" Width="1920"
        Style="{StaticResource WindowWin10}">
    <Grid>
        <hc:TabControl x:Name="tabControl" Margin="5" Loaded="TabControl_OnLoaded" />
    </Grid>
</hc:Window>
